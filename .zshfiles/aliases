#===================== Aliases =====================#
alias hist="history 1"
alias lal="ls -la"
alias du="du -h"
alias df="df -h"
#================== Suffix Aliases =================#
alias -s tex=vim
alias -s txt=vim
alias -s html=firefox
alias -s org=firefox
alias -s com=firefox
alias -s in=firefox
#================= Global Aliases =================#
alias -g ...="../.."
alias -g ....="../../.."
alias -g .....="../../../.."
#==================================================#